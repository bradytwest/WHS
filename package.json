{
  "name": "michigan-custom-calendar",
  "version": "1.0.3",
  "description": "Custom Calendar application made for University of Michigan",
  "main": "index.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "blaise:build": "npm run blaise:build:dev && npm run blaise:build:prod",
    "blaise:build:dev": "npm run blaise:clean:dev && ng build && npm run blaise:build:post",
    "blaise:build:prod": "npm run blaise:clean:prod && ng build --prod && npm run blaise:build:post",
    "blaise:clean": "npm run blaise:clean:dev && npm run blaise:clean:prod",
    "blaise:clean:prod": "rimraf dist/**/*.*",
    "blaise:clean:dev": "rimraf debug/**/*.*",
    "blaise:clean:modules": "rimraf node_modules",
    "blaise:update": "rimraf node_modules/@blaise && npm i",
    "blaise:build:post": "rimraf Datamodel/**/*.bpkg"
  },
  "author": "CBS",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "8.2.9",
    "@angular/common": "8.2.9",
    "@angular/compiler": "8.2.9",
    "@angular/core": "8.2.9",
    "@angular/forms": "8.2.9",
    "@angular/platform-browser": "8.2.9",
    "@angular/platform-browser-dynamic": "8.2.9",
    "@angular/router": "8.2.9",
    "@syncfusion/ej2-angular-base": "17.2.49",
    "@syncfusion/ej2-angular-schedule": "17.2.49",
    "angular2-hotkeys": "2.1.5",
    "bigjs-es6": "1.0.0",
    "classlist.js": "1.1.20150312",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "imask": "5.2.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "moment-es6": "1.0.0",
    "mousetrap": "1.6.3",
    "ng2-dnd": "5.0.2",
    "ng2-nouislider": "1.8.2",
    "ngx-toastr": "10.2.0",
    "nouislider": "14.0.2",
    "rxjs": "6.5.3",
    "zone.js": "0.9.1",
    "@blaise/core": "blaise-core-5.6.5.tgz"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.15",
    "@angular-devkit/build-ng-packagr": "0.803.15",
    "@angular/cli": "8.3.15",
    "@angular/compiler-cli": "8.2.9",
    "@angular/language-service": "8.2.9",
    "@compodoc/compodoc": "1.1.8",
    "@types/jasmine": "3.3.9",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.14",
    "codelyzer": "4.5.0",
    "ng-packagr": "5.3.0",
    "rimraf": "2.6.3",
    "ts-node": "8.4.1",
    "tsickle": "0.34.3",
    "tslib": "1.9.3",
    "tslint": "5.14.0",
    "typescript": "3.5.2"
  }
}
